{
  "name": "Random Password Generator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Random Password Generator",
  "homepage_url": "https://github.com/tmbtech/chrome_extension_random_password",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Random Password Generator",
    "default_popup": "browser_action.html"
  },
  "permissions": [
    "clipboardWrite",
    "storage"
  ]
}